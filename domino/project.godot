config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "ClientNode",
"language": "GDScript",
"path": "res://script/singleton/UDP_SERVER.gd"
}, {
"base": "Node",
"class": "ESP32",
"language": "GDScript",
"path": "res://script/classDefinitions/Esp32.gd"
} ]
_global_script_class_icons={
"ClientNode": "",
"ESP32": ""
}

[application]

config/name="Domino"
run/main_scene="res://scenes/mainLvl.tscn"
config/icon="res://icon.png"

[autoload]

UdpServer="*res://script/singleton/UDP_SERVER.gd"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
